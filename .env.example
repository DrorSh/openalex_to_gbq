# Local folder for raw/converted data (will be symlinked to ./data)
# Leave empty to use ./data directly
DATA_FOLDER=""

# Version suffix for BigQuery table names (format: yyyymmdd)
# Tables will be created as e.g. works_20260210, concepts_20260210
VERSION="20260210"

# Google Cloud project ID
PROJECT_ID="your-project-id"

# GCS bucket for staging data (without gs:// prefix)
GCS_BUCKET="your-bucket-name"

# BigQuery dataset name
BQ_DATASET="openalex"
